[user]
	email = joe23reptiles@gmail.com
	name = joe23rep

 [color]
	ui = true
	branch = auto
	diff = auto
	interactive = auto
	status = auto
  [color "branch"]
    current = #00eeff
    local = #790bf8
    remote = #ff00ff
  [color "diff"]
    meta = #00eeff
    frag = #790bf8
    old = #b6aef4
    new = #45f3d3
  [color "status"]
    added = #00eeff
    changed = #790bf8
    untracked = #ff00ff

[credential]
	helper = cache

[core]
    pager = delta

# [delta]
#     # features = side-by-side decoration unobtrusive-line-numberss
#     features = side-by-side unobtrusive-line-numbers decorations
#     whitespace-error-style = 22 reverse
