#     minus-style = "#b6aef4" italic
#     plus-style = "#9eddff" italic
#     minus-empty-line-marker-style = reverse "#b6aef4"
#     plus-empty-line-marker-style = reverse "#9eddff"
#     minus-emph-style = "#46d9ff"
#     plus-emph-style = "#8400ff"
[delta]
    # features = side-by-side unobtrusive-line-numbers decorations
    features = side-by-side line-numbers decorations
    whitespace-error-style = 22 reverse
	side-by-side = true
    syntax-theme = Necro
    minus-style = "#b6aef4" italic
    plus-style = "#9eddff" italic
    minus-empty-line-marker-style = reverse "#b6aef4"
    plus-empty-line-marker-style = reverse "#9eddff"
    minus-emph-style = "#8400ff" bold
    plus-emph-style = "#46d9ff" bold
    # line-numbers-minus-style = 129
    line-numbers-minus-style = "#ce11f0"
    line-numbers-plus-style = 85
    line-numbers-zero-style =  "#616e88"



